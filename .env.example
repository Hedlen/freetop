# Application Settings
DEBUG=True
APP_ENV=development

# Add other environment variables as needed
TAVILY_API_KEY=tvly-xxx
# JINA_API_KEY=jina_xxx # Optional, default is None

# Browser Instance configuration
# 默认使用 Playwright 内置的 Chromium，避免与用户本地 Chrome 冲突
# 如果设置了 CHROME_INSTANCE_PATH，则使用指定的浏览器路径
CHROME_INSTANCE_PATH=
CHROME_HEADLESS=False

# Proxy configuration
# 代理策略: auto(自动检测), manual(手动配置), direct(直连), smart(智能代理)
CHROME_PROXY_STRATEGY=smart
# 代理服务器地址
# CHROME_PROXY_SERVER=http://127.0.0.1:8080
CHROME_PROXY_USERNAME=
CHROME_PROXY_PASSWORD=
# 代理类型: http, https, socks4, socks5
CHROME_PROXY_TYPE=http
# 自动检测系统代理设置
CHROME_AUTO_DETECT_PROXY=True
# 国内网站直连（智能代理模式）
CHROME_DOMESTIC_DIRECT=True
# 代理白名单（强制使用代理的域名，逗号分隔）
CHROME_PROXY_WHITELIST=google.com,youtube.com,github.com
# 代理黑名单（强制直连的域名，逗号分隔）
CHROME_PROXY_BLACKLIST=baidu.com,taobao.com,weibo.com

# turn off for collecting anonymous usage information
ANONYMIZED_TELEMETRY=false
